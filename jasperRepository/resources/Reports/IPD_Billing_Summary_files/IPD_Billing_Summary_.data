	<parameter name="from_date" class="java.util.Date"/>
	<parameter name="to_date" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
  distinct  so.date_confirm discharge_date, rp.ref patient_id, rp.name, rpa.city_village village, rpa.address3 tehsil, rpa.county_district district, coalesce(rpa.country, 'Non-Tribal') tribal_indicator, so.amount_untaxed actual_cost, so.amount_total amount_charged
FROM sale_order so
  INNER JOIN sale_order_line sol ON so.id = sol.order_id
  INNER JOIN res_partner rp ON so.partner_id = rp.id
  inner join res_partner_address rpa on rp.id = rpa.partner_id
  INNER JOIN product_product pp ON sol.product_id = pp.id
  INNER JOIN product_template pt ON pp.product_tmpl_id = pt.id
  INNER JOIN  product_category pc ON pt.categ_id = pc.id AND lower(pc.name) = 'admit'
WHERE so.date_confirm > $P{from_date} and so.date_confirm < $P{to_date}]]>
	</queryString>
