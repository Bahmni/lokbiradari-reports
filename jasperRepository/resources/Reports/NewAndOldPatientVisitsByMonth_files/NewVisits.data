	<parameter name="to_date" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT t.ageGroup,
       t.male,
       t.female,
       COALESCE(ROUND((t.female/t.male)*1000), t.female*1000) AS sexRatio,
       t.male+t.female AS COUNT
FROM
  (SELECT bar.ageGroup,
          SUM(CASE WHEN bar.gender = 'M' THEN 1 ELSE 0 END) AS male,
          SUM(CASE WHEN bar.gender = 'F' THEN 1 ELSE 0 END) AS female
   FROM
     (SELECT person_id,
                      gender,
						IF(foo.age < 1, '0-1',
						IF(foo.age >= 1 && age < 5, '1-5',
						IF(foo.age >= 5 && age < 16, '5-16',
						IF(foo.age >= 16 && age < 45, '16-45',
						IF(foo.age >= 45 && age < 60, '45-60',
						IF(foo.age >= 60, '64+', 'other')))))) AS ageGroup
      FROM
        (SELECT person_id, birthdate, (YEAR(CURDATE())-YEAR(birthdate)) - (RIGHT(CURDATE(),5) < RIGHT(birthdate,5)) AS age,
                                                                          gender, date_created
         FROM person
         WHERE gender IN ('M','F') ) AS foo
      JOIN encounter e ON e.patient_id = foo.person_id
      WHERE e.encounter_datetime BETWEEN $P{from_date} AND $P{to_date}
	AND Datediff(e.date_created, foo.date_created) < 1) AS bar
   GROUP BY ageGroup) AS t;]]>
	</queryString>
