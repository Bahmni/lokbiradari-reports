	<subDataset name="new_registrations_by_gender_and_pv" uuid="609b7668-e39e-40a2-84a0-0cdd8e91c5b0">
		<parameter name="startDate" class="java.lang.String">
			<defaultValueExpression><![CDATA["01/01/2000"]]></defaultValueExpression>
		</parameter>
		<parameter name="endDate" class="java.lang.String">
			<defaultValueExpression><![CDATA["01/02/2013"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT Date_format(p.date_created, '%Y-%m') month ,
       IF(( Year(p.date_created) - Year(p.birthdate) ) < 12, 'C', p.gender)g,
       IF(jss_program_village is null,'NPV','PV') program_village,
       Count(*)  count
FROM   person p
       INNER JOIN person_address pa
               ON p.person_id = pa.person_id
       LEFT OUTER JOIN jss_program_village pv
                    ON pa.city_village = pv.city_village
WHERE  p.gender IS NOT NULL
       AND p.gender <> '' AND p.date_created > STR_TO_DATE($P{startDate},'%d/%m/%Y') AND p.date_created < STR_TO_DATE($P{endDate},'%d/%m/%Y')
GROUP  BY
          month,
          program_village,
          g
ORDER  BY
          month ASC,
          g ASC]]>
		</queryString>
		<field name="month" class="java.lang.String"/>
		<field name="g" class="java.lang.String"/>
		<field name="program_village" class="java.lang.String"/>
		<field name="count" class="java.lang.Long"/>
	</subDataset>
	<parameter name="startDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[01/01/2000]]></defaultValueExpression>
	</parameter>
	<parameter name="endDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[01/01/2013]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT Date_format(p.date_created, '%Y') year_created ,
       Date_format(p.date_created, '%m') month_created,
       IF(( Year(p.date_created) - Year(p.birthdate) ) < 12, 'C', p.gender)g,
       jss_program_village program_village,
       Count(*)  count
FROM   person p
       INNER JOIN person_address pa
               ON p.person_id = pa.person_id
       LEFT OUTER JOIN jss_program_village pv
                    ON pa.city_village = pv.city_village
WHERE  p.gender IS NOT NULL
       AND p.gender <> ''
GROUP  BY year_created,
          month_created,
          program_village,
          g
ORDER  BY year_created ASC,
          month_created ASC,
          g ASC]]>
	</queryString>
